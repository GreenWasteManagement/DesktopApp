<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="modal" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefWidth="800.0" style="-fx-background-color: #D9D9D9;"
      stylesheets="@../style.css" xmlns="http://javafx.com/javafx/17.0.12"
      fx:controller="com.dashboard.desktopapp.components.BucketsModalController">
    <children>
        <HBox fillHeight="false" VBox.vgrow="NEVER">
            <children>
                <Label text="Visualizar balde">
                    <font>
                        <Font name="System Bold" size="24.0"/>
                    </font>
                </Label>
                <Region maxHeight="0.0" prefHeight="0.0" HBox.hgrow="ALWAYS"/>
            </children>
        </HBox>
        <HBox fillHeight="false" spacing="24.0" VBox.vgrow="NEVER">
            <children>
                <VBox HBox.hgrow="ALWAYS">
                    <children>
                        <Label text="Identificador:">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <TextField fx:id="id" editable="false" style="-fx-border-radius: 15; -fx-background-radius: 15;"
                                   stylesheets="@../style.css">
                            <font>
                                <Font size="20.0"/>
                            </font>
                            <padding>
                                <Insets bottom="8.0" left="10.0" right="10.0" top="8.0"/>
                            </padding>
                        </TextField>
                    </children>
                </VBox>
                <VBox layoutX="10.0" layoutY="10.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label text="Capacidade:">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <TextField fx:id="capacity" editable="false"
                                   style="-fx-border-radius: 15; -fx-background-radius: 15;"
                                   stylesheets="@../style.css">
                            <font>
                                <Font size="20.0"/>
                            </font>
                            <padding>
                                <Insets bottom="8.0" left="10.0" right="10.0" top="8.0"/>
                            </padding>
                        </TextField>
                    </children>
                </VBox>
            </children>
            <VBox.margin>
                <Insets top="24.0"/>
            </VBox.margin>
        </HBox>
        <HBox fillHeight="false" layoutX="42.0" layoutY="221.0" spacing="24.0">
            <children>
                <VBox HBox.hgrow="ALWAYS">
                    <children>
                        <Label text="Está associado?:">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <TextField fx:id="associated" editable="false"
                                   style="-fx-border-radius: 15; -fx-background-radius: 15;"
                                   stylesheets="@../style.css">
                            <font>
                                <Font size="20.0"/>
                            </font>
                            <padding>
                                <Insets bottom="8.0" left="10.0" right="10.0" top="8.0"/>
                            </padding>
                        </TextField>
                    </children>
                </VBox>
                <VBox layoutX="10.0" layoutY="10.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label text="Estado:">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <TextField fx:id="state" editable="false"
                                   style="-fx-border-radius: 15; -fx-background-radius: 15;"
                                   stylesheets="@../style.css">
                            <font>
                                <Font size="20.0"/>
                            </font>
                            <padding>
                                <Insets bottom="8.0" left="10.0" right="10.0" top="8.0"/>
                            </padding>
                        </TextField>
                    </children>
                </VBox>
            </children>
            <VBox.margin>
                <Insets top="24.0"/>
            </VBox.margin>
        </HBox>
        <HBox fillHeight="false" layoutX="42.0" layoutY="421.0" spacing="24.0">
            <children>
                <VBox HBox.hgrow="ALWAYS">
                    <children>
                        <Label text="Munícipe associado:">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <TextField fx:id="municipality" editable="false"
                                   style="-fx-border-radius: 15; -fx-background-radius: 15;"
                                   stylesheets="@../style.css">
                            <font>
                                <Font size="20.0"/>
                            </font>
                            <padding>
                                <Insets bottom="8.0" left="10.0" right="10.0" top="8.0"/>
                            </padding>
                        </TextField>
                    </children>
                </VBox>
            </children>
            <VBox.margin>
                <Insets top="24.0"/>
            </VBox.margin>
        </HBox>
    </children>
    <padding>
        <Insets bottom="32.0" left="32.0" right="32.0" top="32.0"/>
    </padding>
</VBox>
